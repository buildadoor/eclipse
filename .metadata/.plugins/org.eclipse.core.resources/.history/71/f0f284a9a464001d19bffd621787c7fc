	function plus(num01, num02){
		var sum = num01 + num02;
		return sum;
}
 	//ex) 물건가격과 갯수를 매개변수로 전달해서 종합을 리턴하여 
 	//alter()로 로딩하세요 
 		function plus(price, cnt){
		var sum = price * cnt;
		return sum;
		alter(plus(2000,3));
