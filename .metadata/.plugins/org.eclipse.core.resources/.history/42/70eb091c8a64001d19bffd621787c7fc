package jspexp.vo;
//jspexp.vo.DeptClass
public class DeptClass {
	private int deptno;
	private String dname;
	private String loc;
	public DeptClass() {
		super();
		// TODO Auto-generated constructor stub
	}
	
	public DeptClass() {
	}

	public DeptClass(int deptno, String dname, String loc) {
		this.deptno = deptno;
		this.dname = dname;
		this.loc = loc;
	}
	public int getDeptno() {
		return deptno;
	}
	public void setDeptno(int deptno) {
		this.deptno = deptno;
	}
	public String getDname() {
		return dname;
	}
	public void setDname(String dname) {
		this.dname = dname;
	}
	public String getLoc() {
		return loc;
	}
	public void setLoc(String loc) {
		this.loc = loc;
	}
	
	}

